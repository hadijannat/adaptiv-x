{
  "realm": "adaptivx",
  "enabled": true,
  "displayName": "Adaptiv-X",
  "roles": {
    "realm": [
      {
        "name": "demo-admin",
        "description": "Privilege to trigger fault injection"
      }
    ]
  },
  "clients": [
    {
      "clientId": "adaptivx-services",
      "name": "Adaptiv-X Services",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": false,
      "secret": "adaptivx-secret",
      "serviceAccountsEnabled": true,
      "directAccessGrantsEnabled": true,
      "standardFlowEnabled": false,
      "implicitFlowEnabled": false,
      "redirectUris": ["*"],
      "protocolMappers": [
        {
          "name": "audience-adaptivx-services",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-audience-mapper",
          "config": {
            "included.client.audience": "adaptivx-services",
            "id.token.claim": "false",
            "access.token.claim": "true"
          }
        }
      ]
    }
  ],
  "users": [
    {
      "username": "demo-admin",
      "enabled": true,
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "demo-admin",
          "temporary": false
        }
      ],
      "realmRoles": ["demo-admin"]
    }
  ]
}
